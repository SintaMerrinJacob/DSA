 #include <iostream>
 using namespace std;





class Node
{
public:
	int data;
	Node *next;
	Node(int data)
	{
		this->data = data;
		this->next = NULL;
	}
};
// ITERATIVE
int length(Node *head)
{
    if(head == NULL)
        return 0;
    
    int len = 0;
    Node *curr = head;
    while(curr != NULL)
    {
        len++;
        curr = curr -> next;
    }
        
    
    return len;
}

// RECURSIVE
int length(Node *head) 
{
    if(head == NULL)
        return 0;
    if(head->next == NULL)
        return 1;
    
 return 1+length(head->next);
}





Node *takeinput()
{
	int data;
	cin >> data;
	Node *head = NULL, *tail = NULL;
	while (data != -1)
	{
		Node *newNode = new Node(data);
		if (head == NULL)
		{
			head = newNode;
			tail = newNode;
		}
		else
		{
			tail->next = newNode;
			tail = newNode;
		}
		cin >> data;
	}
	return head;
}

int main()
{
	int t;
	cin >> t;
	while (t--)
	{
		Node *head = takeinput();
		cout << length(head) << endl;
	}
	return 0;
}
