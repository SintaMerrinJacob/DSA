class Queue
{
   private :
    int *data;
	int capacity;
	int nextIndex;
	int frontIndex;
	int size;

  public:
  Queue()
  {
	  data = new int[5];
	  capacity = 5;
	  nextIndex = 0;
	  frontIndex = 0;
	  size = 0;
  }

  void enqueue( int x)
  {
       if(isFull())
       {
           cout<<"Overflow"<<endl;
           return;
       }
	     
		
		data[nextIndex] = x;
		nextIndex = (nextIndex+1)%capacity;
		size++;
  } 

  void dequeue()
  {
     if( isEmpty())
     {
          cout<<"Underflow"<<endl;
          return;
     }
	  

	frontIndex = (frontIndex+1)%capacity;
	size--;
	
	if(size == 0)
	{
	     nextIndex = 0;
	  frontIndex = 0;
	}
	
 }

  void getfront()
  {
      if(isEmpty())
      {
          return;
      }
    cout<< data[frontIndex]<<endl;
  }

  void getRear()
  {
        cout<<data[(frontIndex+size-1)%capacity]<<endl;
  }

  void getsize()
  {
        cout<<"Size is "<< size<<endl;
  }

  bool isEmpty()
  {
        return (size == 0);
		  
  }
  bool isFull()
  {
	  return( capacity == size);
	    
  }

  
};

