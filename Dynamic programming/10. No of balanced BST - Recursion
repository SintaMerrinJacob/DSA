int balancedBTs(int n) {
    // Write your code here
    // base case 
    if(n == 0 or n == 1) {
        return 1;
    }
    
    long smalloutput1 = balancedBTs(n - 1);
    long smalloutput2 = balancedBTs(n - 2);
    
    return ((smalloutput1 % mod) * ((2 * smalloutput2) + smalloutput1) % mod) % mod;
}
