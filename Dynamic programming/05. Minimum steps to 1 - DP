
int countStepsToOne(int n)
{
	vector<int>minCount(n+1);
    minCount[1] = 0;
    
    for(int i = 2 ; i<=n ; i++)
    {
        int x = minCount[i-1];
        int y = INT_MAX , z = INT_MAX;
        
        if( i%2 == 0)
            y = minCount[i/2];
        
        if(i%3 == 0)
            z = minCount[i/3];
        
        minCount[i] = min( x , min(y,z))+1;
            
    }
    
  return  minCount[n];
    
    
}
